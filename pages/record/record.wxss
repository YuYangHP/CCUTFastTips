.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:30%;
    justify-content: center;
    text-align: center;
    font-size: 15px;

}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 37%;
  justify-content: center;
  background: #5e9beb;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 17px;
}